import math

# a ve b'nin hesaplanması
a = (81**0.25) * math.cos(5) - 2**(-0.36) * math.sin(5)
b = math.sin(math.3*tan(4) * math.cos(math.sqrt(3)))

# m'nin hesaplanması
ab = a * b

if ab > 0.5:
    m = math.exp(-(abs(a) - abs(b))) / (a**2 + b**2))
else:
    m = abs(a + b)
print(a)
print(b)
print(m)



